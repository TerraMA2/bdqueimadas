<div class="left-content-box" id="filter-box">
  <div class="form-group">
    <label for="exampleInputEmail1">Temporal</label>
    <input type="text" class="form-control date filter-date" id="filter-date-from" placeholder="De" data-inputmask="'alias': 'dd/mm/yyyy'" data-mask>
  </div>
  <div class="form-group">
    <input type="text" class="form-control date filter-date" id="filter-date-to" placeholder="At&eacute;" data-inputmask="'alias': 'dd/mm/yyyy'" data-mask>
  </div>
  <div class="form-group">
    <label>Por Sat√©lite</label>
    <select class="form-control" id="filter-satellite">
      <option value="all">TODOS</option>
      <%
      filterConfig.SatellitesList.forEach(function(satellite, index) {
        if(satellite.Current) {
        %>
        <option value="<%- satellite.Name %>"><%- satellite.Name %></option>
        <%
        }
      })
      %>
    </select>
  </div>
  <button class="btn btn-primary" id="filter-button">Aplicar</button>
</div>
